{% block javascripts %}
    {% javascripts '@AddressBundle/Resources/public/js/*' output='compiled/js/address.js' %}
           <script src="{{ asset_url }}"></script>
    {% endjavascripts %}  
{% endblock %}
<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <h4 class="login-box-msg">
            Manter endereços
        </h4>
    </div>
</div>
<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <button data-target="#modal_address_map" data-toggle="modal" type="button" class="btn btn-danger pull-right">Visualizar mapa</button>
        <button data-target="#modal_address_add" data-toggle="modal" type="button" class="btn btn-primary pull-left">Adiconar endereço</button>
    </div>
</div>
<hr>    
<div class="modal fade" id="modal_address_add" tabindex="-1" role="dialog"    >
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            {% include '@Address/Forms/address.html.twig' %}
        </div><!-- /modal-content -->
    </div><!-- /modal-dialog -->
</div><!-- /modal_address_add -->

<div class="modal fade" id="modal_address_edit" tabindex="-1" role="dialog">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            
        </div><!-- /modal-content -->
    </div><!-- /modal-dialog -->
</div><!-- /modal_address_add -->